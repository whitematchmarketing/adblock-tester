    "symbol-observable": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "prerender": {
    "routes": [
      "/"
    ],
    "rendererConfig": {
      "renderAfterDocumentEvent": "prerender-trigger"
    }
  },
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "**",
        "staticPath": "static"
      },
      {
        "outDirPattern": "**/checkadblock.ru",
        "staticPath": "static-ru"
      },
      {
        "outDirPattern": "**/adblock-tester.com",
        "staticPath": "static-en"
      }
    ]
  }
