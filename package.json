  "scripts": {
    "start": "yarn dev",
    "dev": "./dev.sh ru",
    "dev:en": "./dev.sh en",
    "build": "./build.sh",
    "deploy": "./deploy.sh",
    "convertHtmls": "node ./transformHtmlIcludes.js",
    "postinstall": "node ./postinstall.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.0",
    "core-js": "^3.23.1",
    "debug": "^4.1.1",
    "effector": "^20.9.1",
    "flat": "^5.0.0",
    "loadjs": "^4.2.0",
    "ms": "^2.1.3",
