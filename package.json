    "parcel-plugin-interpolate-html": "^1.0.1",
    "parcel-plugin-prerender": "^1.4.1",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "parcel-plugin-svelte": "^4.0.5",
    "postcss-load-config": "^2.1.0",
    "postcss-modules": "^1.4.1",
    "sass": "^1.52.3",
    "svelte": "^3.16.7",
    "svelte-preprocess": "^3.3.0",
