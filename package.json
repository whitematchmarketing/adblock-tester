{
  "name": "checkadblock",
  "version": "1.0.0",
  "repository": "git@github.com:ymatuhin/checkadblock.git",
  "author": "ymatuhin <ym@ymatuhin.ru>",
  "license": "MIT",
  "devDependencies": {
    "bettertext.css": "^2.0.4",
    "http-server-ssl": "^0.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-interpolate-html": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "pug": "^2.0.4",
    "sass": "^1.24.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "parcel ./src/index.pug --out-dir ./docs --no-cache",
    "dev": "parcel serve ./src/index.pug --out-dir ./docs --https --no-cache",
    "build": "NODE_ENV=test parcel build ./src/index.pug --out-dir ./docs --no-cache",
    "prestart": "yarn clean_docs",
    "predev": "yarn clean_docs",
    "prebuild": "yarn clean_docs",
    "clean_docs": "rm -rf ./docs/**"
  }
}
