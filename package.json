{
  "name": "checkadblock",
  "version": "3.0.5",
  "repository": "git@github.com:ymatuhin/checkadblock.git",
  "author": "ymatuhin <ym@ymatuhin.ru>",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.2",
    "autoprefixer": "^9.7.3",
    "http-server-ssl": "^0.0.7",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-interpolate-html": "^1.0.1",
    "parcel-plugin-prerender": "^1.4.1",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "parcel-plugin-svelte": "^4.0.5",
    "postcss-load-config": "^2.1.0",
    "postcss-modules": "^1.4.1",
    "sass": "^1.24.0",
    "svelte": "^3.16.7",
    "svelte-preprocess": "^3.3.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "yarn dev",
    "dev": "NODE_ENV=development LANG=ru parcel serve ./src/index.html --out-dir ./dev --https --no-cache --log-level 3",
    "build": "NODE_ENV=production LANG=ru parcel build ./src/index.html --out-dir ./docs --no-cache --detailed-report --no-source-maps",
    "predev": "npm run clearCache",
    "postbuild": "grep -q 'https://localhost:1234' ./docs/index.html && exit 1 || echo 'builded with correct env'",
    "prebuild": "npm run updateSizes && npm run clearCache && rm -rf ./docs/**",
    "postversion": "sed \"s/VERSION=.*/VERSION=$npm_package_version/\" .env | tee .env",
    "updateSizes": "node ./nodeFetchSizes.js",
    "convertHtmls": "node ./transformHtmlIcludes.js",
    "postinstall": "npm run clearCache && node ./postinstall.js",
    "clearCache": "rm -rf ./.cache",
    "fixsvelte": "npm i file:./parcel-plugin-force-babel"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.0",
    "debug": "^4.1.1",
    "effector": "^20.9.1",
    "flat": "^5.0.0",
    "loadjs": "^4.2.0",
    "normalizr": "^3.4.1",
    "whatwg-fetch": "^3.0.0"
  },
  "prerender": {
    "routes": [
      "/"
    ],
    "rendererConfig": {
      "renderAfterDocumentEvent": "prerender-trigger"
    }
  }
}
